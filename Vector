#include <iostream>
#include <vector>
using namespace std;
int sumsquare_list(vector <int> sqr)
{
int sum = 0;
for(int i=0; i<sqr.size(); i++)
{
  sum = sum + (sqr[i] * sqr[i]);
}
return sum;
}
int main()
{
  vector <int> sqr (5);
  sqr [0] = 1;
  sqr [1] = 2;
  sqr [2] = 3;
  sqr [3] = 4;
  sqr [4] = 5;
      cout <<"The sum of squares from 1 to 5 is: " << sumsquare_list(sqr)<< endl;
return 0;
}
